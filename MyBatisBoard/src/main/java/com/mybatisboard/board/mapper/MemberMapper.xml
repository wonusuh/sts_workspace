<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mybatisboard.board.mapper.MemberMapper">

	<select id="getMemberList"
		resultType="com.mybatisboard.board.entity.Member">
		SELECT *
		FROM members
		order by memIdx desc;
	</select>

	<select id="checkMember"
		resultType="com.mybatisboard.board.entity.Member">
		SELECT *
		FROM members
		WHERE memId = #{memId}
		AND memPassword =
		#{memPassword};
	</select>

</mapper>
